<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.Outermatrix-E_Entertainment"
       name="E! Entertainment"
       version="0.6.9"
       provider-name="outermatrix">
  <requires>
    <import addon="xbmc.python" version="1.0"/>
    <import addon="script.module.beautifulsoup" version="3.0.8"/>
    <import addon="script.module.demjson" version="1.4"/>
    <import addon="script.module.mechanize" version="0.2.5"/>
    <import addon="script.module.amazon.database" version="0.0.1"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
             library="default.py">
	<provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <summary>E! Entertainment Television</summary>
    <description>Movies and Television Shows</description>
    <disclaimer>Some parts of this addon may not be legal in your country of residence - please check with your local laws before installing.</disclaimer>
  </extension>
</addon>

<addon id="plugin.video.Outermatrix-Smithsonian"
       name="Smithsonian Channel"
       version="1.0.1"
       provider-name="Outermatrix">
  <requires>
    <import addon="xbmc.python" version="1.0"/>
    </requires>
  <extension point="xbmc.python.pluginsource"
            library="default.py">
  <provides>video audio</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>Smithsonian television shows</summary>
    <description>Smithosonian channels</description>
    <platform>all</platform>
  </extension>
</addon>
</addons>